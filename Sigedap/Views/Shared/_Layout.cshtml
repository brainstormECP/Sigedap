    <script src="~/Scripts/jquery-ui-1.11.2.min.js"></script>
    <script src="~/Content/tema/js/jquery.prettyPhoto.js"></script>
    <script src="~/Content/tema/js/wow.min.js"></script>
    <script src="~/Content/tema/js/jquery.isotope.min.js"></script>
    <script src="~/Content/tema/js/bootstrap.min.js"></script>
    <script src="~/Content/tema/js/main.js"></script>
    <script>
        var sigedap = angular.module("sigedap", []);
        $(document).ready(function () {
            $(".datepicker").datepicker({
                dateFormat: "dd/mm/yy",
                monthNames: ["Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio",
                    "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Diciembre"],
                monthNamesShort: ["Ene", "Feb", "Mar", "Abr", "May", "Jun", "Jul", "Ago", "Sep", "Oct", "Nov", "Dic"],
                dayNamesMin: ["Do", "Lu", "Ma", "Mi", "Ju", "Vi", "Sa"]
            });
        })
    </script>
