                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Reclamaciones<i class="fa fa-angle-down"></i></a>
                            <ul class="dropdown-menu">
                                <li><a href="@Url.Action("Create", "Reclamacion" )">Nueva Reclamación </a></li>
                                <li><a href="@Url.Action("Index", "Reclamacion" )">Reclamaciones Existentes </a></li>
                                <li><a href="@Url.Action("Index", "DictamenDeReclamacion")">Dictamenes de Reclamaciones</a></li>

                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Expedientes <i class="fa fa-angle-down"></i></a>
                            <ul class="dropdown-menu">
                                <li><a href="@Url.Action("Index", "Expediente" )">Abiertos</a></li>
                                <li><a href="#">Cerrados</a></li>
                                <li><a href="#">...</a></li>
                                <li><a href="#">...</a></li>
                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Reportes Estadísticos <i class="fa fa-angle-down"></i></a>
                            <ul class="dropdown-menu">
                                <li><a href="#">... </a></li>
                                <li><a href="#">... </a></li>
                                <li><a href="#">...</a></li>
                                <li><a href="#">...</a></li>
                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Trabajadores <i class="fa fa-angle-down"></i></a>
                            <ul class="dropdown-menu">
                                <li><a href="@Url.Action("Index", "Operador" )">Operadores</a></li>
