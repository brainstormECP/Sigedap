                                <li><a href="@Url.Action("Index", "Marca" )">Marcas</a></li>
                                <li><a href="@Url.Action("Index", "Cliente")">Clientes</a></li>
                                <li><a href="@Url.Action("Index", "Configuracion")">Configuración</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div><!--/.container-->
        </nav><!--/nav-->

    </header><!--/header-->
    @RenderBody()

    <footer id="footer" class="midnight-blue navbar-fixed-bottom up">
        <div class="container">
            <div class="row">
                <div class="col-sm-6">
                    <p title="">  &copy;  {{@DateTime.Now.Year }} OBEP Jovellanos. {{ 2 + 3}}</p>
                </div>
                <div class="col-sm-6">
                    @*<ul class="pull-right">
                            <li><a href="#">Home</a></li>
                            <li><a href="#">About Us</a></li>
                            <li><a href="#">Faq</a></li>
                            <li><a href="#">Contact Us</a></li>
                        </ul>*@
