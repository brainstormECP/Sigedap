@model Sigedap.Models.DictamenDeReclamacion

@{
    ViewBag.Title = "Nuevo";
}


<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="section-title">
                <h2 class="text-center"><strong>Nuevo Dictamen De Reclamación</strong></h2>
                <div class="line">
                    <hr>
                </div>
                <div class="clearfix"></div>
            </div>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="row">
                    @Html.ValidationSummary(true)

                    <div class="col-md-6">
                        @Html.LabelFor(model => model.Ueb, new { @class = "control-label col-md-6" })
                        <div class="form-group">
                            @Html.EditorFor(model => model.Ueb)
                            @Html.ValidationMessageFor(model => model.Ueb)
                        </div>
                    </div>

                    <div class="col-md-6">
                        @Html.LabelFor(model => model.OficinaComercial, new { @class = "control-label col-md-6" })
                        <div class="form-group">
                            @Html.EditorFor(model => model.OficinaComercial)
                            @Html.ValidationMessageFor(model => model.OficinaComercial)
                        </div>
                    </div>

                    <div class="col-md-6">
                        @Html.LabelFor(model => model.Procede, new { @class = "control-label col-md-6" })
                        <div class="form-group">
                            @Html.EditorFor(model => model.Procede)
                            @Html.ValidationMessageFor(model => model.Procede)
                        </div>
                    </div>

                    <div class="col-md-6">
                        @Html.LabelFor(model => model.Observaciones, new { @class = "control-label col-md-6" })
                        <div class="form-group">
                            @Html.EditorFor(model => model.Observaciones)
                            @Html.ValidationMessageFor(model => model.Observaciones)
                        </div>
                    </div>

                    <div class="col-md-6">
                        @Html.LabelFor(model => model.RevisadoPor, new { @class = "control-label col-md-6" })
                        <div class="form-group">
                            @Html.EditorFor(model => model.RevisadoPor)
                            @Html.ValidationMessageFor(model => model.RevisadoPor)
                        </div>
                    </div>

                    <div class="col-md-6">
                        @Html.LabelFor(model => model.AutorizadoPor, new { @class = "control-label col-md-6" })
                        <div class="form-group">
                            @Html.EditorFor(model => model.AutorizadoPor)
                            @Html.ValidationMessageFor(model => model.AutorizadoPor)
                        </div>
                    </div>

                    <div class="col-md-6">
                        @Html.LabelFor(model => model.FechaNotificacionAlCliente, new { @class = "control-label col-md-6" })
                        <div class="form-group">
                            @Html.EditorFor(model => model.FechaNotificacionAlCliente)
                            @Html.ValidationMessageFor(model => model.FechaNotificacionAlCliente)
                        </div>
                    </div>

                    <div class="col-md-6">
                        @Html.LabelFor(model => model.NombreReclamante, new { @class = "control-label col-md-6" })
                        <div class="form-group">
                            @Html.EditorFor(model => model.NombreReclamante)
                            @Html.ValidationMessageFor(model => model.NombreReclamante)
                        </div>
                    </div>

                    <div class="col-md-6">
                        @Html.LabelFor(model => model.CiReclamante, new { @class = "control-label col-md-6" })
                        <div class="form-group">
                            @Html.EditorFor(model => model.CiReclamante)
                            @Html.ValidationMessageFor(model => model.CiReclamante)
                        </div>
                    </div>

                    <div class="col-md-6">
                        @Html.LabelFor(model => model.ClienteId, "ClienteId", new { @class = "control-label col-md-6" })
                        <div class="form-group">
                            @Html.DropDownList("ClienteId", null, String.Empty, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.ClienteId)
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-offset-10 col-md-2">
                            @Html.ActionLink("Cerrar", "Index", null, new { @class = "btn btn-default" })
                            <input type="submit" value="Guardar" class="btn btn-success" />
                        </div>
                    </div>
                </div>
                <hr />
            }
        </div>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
