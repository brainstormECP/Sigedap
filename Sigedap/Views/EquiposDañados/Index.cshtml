@model IEnumerable<Sigedap.Models.EquipoDañado>

@{
    ViewBag.Title = "Index";
}
<div class = "panel panel-default">
<div class = "panel-heading">
<p>
    @Html.ActionLink("Nuevo EquipoDañado", "Create",null, new {@class = "btn btn-success"})
</p>
</div>
<div class = "panel-body">
<table class="table dynamic">
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ActaDevolucion.Representante)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActaEntregaEquiposReposicion.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActasRecogidaDestruir.Testigo2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Marca.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nacionalidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Serie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Modelo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExpedienteDictamenDeReclamacionId)
        </th>
        <th></th>
    </tr>
</thead>
<tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ActaDevolucion.Representante)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActaEntregaEquiposReposicion.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActasRecogidaDestruir.Testigo2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Marca.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nacionalidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Serie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Modelo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExpedienteDictamenDeReclamacion.Numero)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }, new {@class = "btn btn-info"}) |            
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id }, new {@class = "btn btn-danger"})
        </td>
    </tr>
}
<tbody>
</table>
</div>
</div>

@section Scripts {
    <script src="~/Scripts/jquery.dataTables.js"></script>
    <script src="~/Scripts/mbs-qdtf.js"></script>
}

